{
  "name": "wordhunt",
  "version": "1.0.0",
  "description": "GamePidgeon Word Hunt!",
  "browserslist": "defaults",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "parcel build html/*.html",
    "check": "tsc",
    "doc": "typedoc --entryPointStrategy expand src/ types/",
    "format": "prettier --write html/ src/ styles/ tests/",
    "start": "parcel html/*.html",
    "test": "jest"
  },
  "author": "Stephen Xu",
  "license": "UNLICENSED",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@parcel/transformer-inline-string": "^2.9.1",
    "cypress": "^13.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.6.2",
    "json-schema-to-typescript": "^13.0.1",
    "parcel": "^2.9.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "whatwg-fetch": "^3.6.17"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "ajv": "^8.12.0"
  }
}
